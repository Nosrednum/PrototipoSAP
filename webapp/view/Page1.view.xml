<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.untitledPrototype.controller.Page1" xmlns="sap.m" xmlns:layout="sap.ui.commons.layout">
    <Page showHeader="false" title="Page Title" showFooter="false" showNavButton="false" enableScrolling="false">
        <content>
            <layout:AbsoluteLayout horizontalScrolling="Hidden" verticalScrolling="Auto">
                <layout:positions>
                    <layout:PositionContainer left="961px" top="584px">
                        <layout:control>
                            <Image src="/webapp/resources/logo.png" height="259px" width="259px" visible="true" densityAware="false"/>
                        </layout:control>
                    </layout:PositionContainer>
                    <layout:PositionContainer left="226px" top="255px">
                        <layout:control>
                            <GenericTile header="Facturas" subheader="Ir a las facturas de la empresa" frameType="OneByOne" press="_onGenericTilePress">
                                <tileContent>
                                    <TileContent footer="Todas las Facturas" unit="COP">
                                        <content>
                                            <NumericContent value="24" valueColor="Neutral" indicator="None" icon="sap-icon://money-bills"/>
                                        </content>
                                    </TileContent>
                                </tileContent>
                            </GenericTile>
                        </layout:control>
                    </layout:PositionContainer>
                    <layout:PositionContainer left="599px" top="257px">
                        <layout:control>
                            <GenericTile header="Pago Facturas" subheader="Or a pagar una factura" frameType="OneByOne" press="_onGenericTilePress1">
                                <tileContent>
                                    <TileContent footer="Pago facturas" unit="COP">
                                        <content>
                                            <NumericContent value="2" valueColor="Neutral" indicator="None" icon="sap-icon://per-diem"/>
                                        </content>
                                    </TileContent>
                                </tileContent>
                            </GenericTile>
                        </layout:control>
                    </layout:PositionContainer>
                </layout:positions>
            </layout:AbsoluteLayout>
        </content>
        <footer>
            <OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
                <content>
                    <ToolbarSpacer width=""/>
                    <Button text="Save" type="Emphasized" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
                    <Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
                </content>
            </OverflowToolbar>
        </footer>
        <headerContent/>
        <subHeader/>
        <customHeader/>
    </Page>
</mvc:View>
